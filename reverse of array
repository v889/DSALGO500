#include <stdio.h>

int main()
{
    int n,i,j,ptr[100];
    printf("enter lenght of array:");
    scanf("%d",&n);
    printf("element of array:\n");
    for(i=1;i<n;i++)
    {
        scanf("%d",&ptr[i]);
    }
    printf("\nyour entered array:\n");
    for(i=0;i<n;i++)
    {
        printf("%d\t",ptr[i]);
    }
    printf("\nreverse array:\n");
    for(i=n;i>1;i--)
    {
        printf("%d\t",ptr[i]);
    }
    
    
}
